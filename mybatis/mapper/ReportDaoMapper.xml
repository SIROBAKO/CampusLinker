<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hako.web.cl.dao.CLReportDao">



	<resultMap type="com.hako.web.cl.entity.CL_Report"
		id="Result_Report">
	</resultMap>

	<parameterMap type="com.hako.web.cl.entity.CL_Report"
		id="Param_Report">
	</parameterMap>

	<insert id="insert" parameterMap="Param_Report">
		INSERT INTO
		CL_REPORT(REPORT_USER,PURPOSE,PURPOSE_NUM,PURPOSE_DATA)
		VALUES(#{report_user},#{purpose},#{purpose_num},#{purpose_data})
	</insert>




</mapper>
